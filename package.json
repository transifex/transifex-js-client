{
  "name": "transifex-js-client",
  "version": "1.0.0",
  "description": "Wrapper around Transifex's API on node and Browser",
  "main": "dist/transifex-api.min.js",
  "repository": "git@github.com:transifex/transifex-js-client.git",
  "author": "Alex Psi <alexpsi256@gmail.com>",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "./scripts/deploy",
    "prebuild": "rimraf dist",
    "build": "webpack --env.prod -p",
    "prebuild:dev": "rimraf dist",
    "build:dev": "webpack --env.dev",
    "test": "rimraf coverage && karma start",
    "jsdoc": "node_modules/jsdoc/jsdoc.js -r src/ -d docs --readme readme.md",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "validate": "npm-run-all --parallel lint build test",
    "coveralls": "cat coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint ./src"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-__coverage__": "^11.0.0",
    "babel-preset-es2015": "^6.22.0",
    "bluebird": "^3.4.7",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coveralls": "^2.11.16",
    "cpy-cli": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-promise": "^3.4.1",
    "eslint-plugin-standard": "^2.0.1",
    "jsdoc": "^3.4.3",
    "karma": "^1.4.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-env-preprocessor": "^0.1.1",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.1",
    "opt-cli": "^1.5.1",
    "phantomjs-polyfill": "^0.0.2",
    "rimraf": "^2.5.4",
    "shortid": "^2.2.6",
    "webpack": "^2.2.1",
    "webpack-config-utils": "^2.3.0"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "blueimp-md5": "^2.6.0",
    "extend": "^3.0.0"
  }
}
